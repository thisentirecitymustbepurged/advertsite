{
  "extends": "airbnb",
  "rules": {
    "func-names": ["error", "never"],
    "linebreak-style": 0,
    "no-fallthrough": 0,
    "react/prop-types": 0,
    "jsx-a11y/no-redundant-roles": 0,
    "arrow-parens": 0,
    "consistent-return": 0,
    "no-func-assign": 0,
    "no-param-reassign": 0,
    "react/no-array-index-key": 0,
    "class-methods-use-this": 0,
    "react/jsx-no-bind": 0,
    "comma-dangle": 0,
  }
}